{
  "manifest_version": 2,
  
  "name": "Zoom meeting participants",
  "description": "It will return the list of  all participants in an meeting",
  "version": "1.0",
  
  "permissions": [
    "activeTab"
  ],
  
  "browser_action": {
    "default_popup": "popup.html"
  },
  
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  }
}