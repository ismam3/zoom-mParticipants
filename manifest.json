{
  "manifest_version": 2,
  
  "name": "Zoom meeting participants",
  "description": "It will return the list of  all participants in an zoom meeting",
  "version": "1.0",
  "version_name":"beta",
  
  "permissions": [
    "activeTab"
  ],
  "icons":{
    "16":"icons/16.png",
    "48":"icons/48.png",
    "128":"icons/128.png"
  },
  
  "browser_action": {
    "default_popup": "popup.html"
  },
  
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  }
}